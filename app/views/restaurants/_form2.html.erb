
<% #, :html => { :multipart => true} %>
<%= form_for @food, :url => {:action => :addfood}, :html => {:multipart => true}  do |f| %>
<%= f.label :name %>	
	<%= f.text_field :name %>
	<br/ >
	<%= f.label :price %>
	<%= f.text_field :price %>
	<br />
	<%= f.label :description %>
	<%= f.text_field :description %>
	<br />
	<%= f.label :avatar %>
	<%= f.file_field :avatar %>
	<br />
	<%= f.submit 'Save' %>
<% end %>